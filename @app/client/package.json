{
  "name": "@app/client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gql:build": "yarn gql:build:codegen && yarn gql:build:tsc",
    "gql:build:essential": "yarn run gql:build",
    "gql:watch": "touch index.ts && concurrently --kill-others --names 'codegen,tsc' --prefix-colors 'blue,yellow' 'yarn gql:watch:codegen' 'yarn gql:watch:tsc'",
    "gql:build:codegen": "graphql-codegen --config codegen.yml",
    "gql:build:tsc": "tsc",
    "gql:watch:codegen": "yarn gql:build:codegen --watch",
    "gql:watch:tsc": "yarn gql:build:tsc --watch",
    "dev:ssr": "ng run edmflare:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run edmflare:server:production",
    "prerender": "ng run edmflare:prerender"
  },
  "private": true,
  "dependencies": {
